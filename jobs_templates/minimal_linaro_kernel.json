{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "dtb": "https://storage.kernelci.org/mainline/v4.10-rc8-47-g0722f57bfae9/arm-sunxi_defconfig/dtbs/sun8i-h3-orangepi-pc.dtb",
                "kernel": "https://storage.kernelci.org/mainline/v4.10-rc8-47-g0722f57bfae9/arm-sunxi_defconfig/zImage",
                "overlays": ["https://storage.kernelci.org/mainline/v4.10-rc8-47-g0722f57bfae9/arm-sunxi_defconfig/modules.tar.xz"],
                "ramdisk": "http://storage.kernelci.org/images/rootfs/buildroot/armel/rootfs.cpio.gz"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "device_type": "sun8i-h3-orangepi-pc",
    "job_name": "custom_job",
    "logging_level": "DEBUG",
    "priority": "high",
    "timeout": 600
}
