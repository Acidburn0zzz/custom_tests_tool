{
    "actions": [
        {
            "command": "deploy_linaro_kernel",

            "parameters": {
                "dtb": "https://storage.kernelci.org/mainline/v4.10-11319-gc82be9d2244a/arm-multi_v7_defconfig/dtbs/am335x-boneblack.dtb",
                "kernel": "https://storage.kernelci.org/mainline/v4.10-11319-gc82be9d2244a/arm-multi_v7_defconfig/zImage",
                "ramdisk": "https://storage.kernelci.org/images/rootfs/buildroot/armel/rootfs.cpio.gz",
                "overlays": [
                    "https://storage.kernelci.org/mainline/v4.10-11319-gc82be9d2244a/arm-multi_v7_defconfig/modules.tar.xz"
                ],
                "login_prompt": "buildroot login:",
                "username": "root"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_urls": [ ],
                "timeout": 900,
                "skip_install": "all"
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "device_type": "beaglebone-black",
    "job_name": "custom_job",
    "logging_level": "DEBUG",
    "priority": "high",
    "timeout": 600
}
