{
    "actions": [
        {
            "command": "deploy_linaro_kernel",

            "parameters": {
                "dtb": "/path/to/dtb",
                "kernel": "/path/to/kernel",
                "ramdisk": "/path/to/rootfs.cpio.gz",
                "overlays": [
                    "/path/to/modules.tar.xz"
                ],
                "login_prompt": "buildroot login:",
                "username": "root"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "lava_command_run",
            "parameters": {
                "commands": [
                    "/tests/run.sh"
                ],
                "timeout": 900
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "device_type": "my-device-type",
    "job_name": "my-custom-name",
    "logging_level": "DEBUG",
    "priority": "high",
    "timeout": 600
}
